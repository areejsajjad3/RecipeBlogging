        #home-bg-img {
            position: fixed;
            top: 50%;
            left: 50%;
            min-width: 100%;
            min-height: 100%;
            transform: translateX(-50%) translateY(-50%);
            height: auto;
            z-index: -1;
        }
        
        #home-content {
            width: 100%;
            height: 100%;
            display: table;
            margin-top: 57px;
        }
        
        #home-content-inner {
            display: table-cell;
            vertical-align: middle;
        }
        
