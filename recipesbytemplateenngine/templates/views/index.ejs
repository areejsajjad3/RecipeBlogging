        }
        
        .btn-general {
            font-family: tahoma;
            font-size: 9px;
            margin: 0 6px;
            padding: 12px 46px 12px 46px;
            transition: all 0.4s;
            color: white;
            border-radius: 95px;
            background-color: #f4c613;
        }
        
        .btn-warning {
            font-family: tahoma;
            font-size: 2px;
